{
  "env": {
	"browser": true,
	"es6": true
  },
  "extends": [
	"plugin:@wordpress/eslint-plugin/recommended",
	"plugin:prettier/recommended"
  ],
  "parserOptions": {
	"sourceType": "module",
	"requireConfigFile": false
  },
  "rules": {
	"@wordpress/no-global-event-listener": "off",
	"prettier/prettier": [
	  "error",
	  {
		"singleQuote": true,
		"trailingComma": "es5"
	  }
	]
  }
}
